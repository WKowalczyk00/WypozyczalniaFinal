                  </HBox>
                  <HBox prefHeight="37.0" prefWidth="640.0">
                     <children>
                        <VBox prefHeight="37.0" prefWidth="130.0">
                           <children>
                              <Label prefHeight="34.0" prefWidth="143.0" text="DOSTĘPNOŚĆ" />
                           </children>
                        </VBox>
                        <RadioButton fx:id="dostepneRadioButton" mnemonicParsing="false" prefHeight="33.0" prefWidth="111.0" text="DOSTĘPNE" />
                        <RadioButton fx:id="nieDostepneRadioButton" mnemonicParsing="false" prefHeight="33.0" prefWidth="111.0" text="NIEDOSTĘPNE" />
                     </children>
                  </HBox>
                  <HBox prefHeight="111.0" prefWidth="640.0">
                     <children>
                        <VBox prefHeight="111.0" prefWidth="175.0" />
                        <VBox prefHeight="111.0" prefWidth="135.0">
                           <children>
                              <Button fx:id="dodajButton" mnemonicParsing="false" prefHeight="117.0" prefWidth="134.0" text="DODAJ AUTO" />
                           </children>
                        </VBox>
                        <VBox prefHeight="111.0" prefWidth="144.0">
                           <children>
                              <Button fx:id="anulujButton" mnemonicParsing="false" prefHeight="112.0" prefWidth="142.0" text="ANULUJ" />
                           </children>
                        </VBox>
                        <VBox prefHeight="111.0" prefWidth="169.0" />
                     </children>
                  </HBox>
               </children>
